#JSGF V1.0;

grammar robotas;

public <COMMAND> = <EIK> | <SUK>|<UGNIS>|<STOK>|<PRIESAS>;
<EIK> = (VAŽIUOK | VARYK ) [ ( VIENĄ ŽINGSNĮ | DU ŽINGIUS | TRIS ŽINGIUS | KETURIS ŽINGIUS | PENKIS ŽINGIUS) ] (PIRMYN | ATGAL);
<SUK> = (SUK | GRĘŽKIS ) ( KAIRĖN | DEŠINĖN ) [(TREČIA | ŠEŠTA | DEVINTA | DVYLIKTA) VALANDA];
<UGNIS> = PAVIENIAIS UGNIS;
<STOK> = STOK DABAR;
<PRIESAS> = STEBĖK PRIEŠĄ (TREČIA | ŠEŠTA | DEVINTA | DVYLIKTA) VALANDA;
